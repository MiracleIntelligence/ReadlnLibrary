                                <Run Text="by"/>
                                <Run Text="{Binding Author}"/>
                                    </TextBlock>
                                </StackPanel>
                            </Grid>
                            </Button>
                        </DataTemplate>
                    </controls:ItemsRepeater.ItemTemplate>
                </controls:ItemsRepeater>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
